{
    "name": "susilon/bantingan3",
    "description": "Bantingan MVC framework V3, with PHP8 support",
    "type": "project",
    "require": {
        "php": "^7.2",
        "symfony/routing": "^5.2",
        "smarty/smarty": "^4.2",
        "gabordemooij/redbean": "dev-master",
        "dompdf/dompdf": "^2.0",
        "phpoffice/phpspreadsheet": "^1.16",
        "phpmailer/phpmailer": "^6.2",
        "gregwar/captcha": "^1.2",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "classmap": ["bantingan"],
        "psr-4": {
            "Controllers\\" : "controllers",
            "Models\\" : "models",
            "Modules\\" : "modules"
        }
    },
